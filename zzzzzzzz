        game:GetService("VirtualInputManager"):SendKeyEvent(true,"Space",false,game)
        game:GetService("VirtualInputManager"):SendKeyEvent(true,"Space",false,game)
        game:GetService("VirtualInputManager"):SendKeyEvent(false,"Space",false,game)
end
