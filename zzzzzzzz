Tabs.Main:AddButton({
    Title = "Test Webhook",
    Callback = function()
        local Headers = {
            ['Content-Type'] = 'application/json',
        }
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local banned = LocalPlayer:GetAttribute("banned")
        local bannedStatus
        if banned == true then
            bannedStatus = "✅"
        elseif banned == false then
            bannedStatus = "❌"
        else
            bannedStatus = "Không xác định"
        end

        local data = {
            ["embeds"] = {
                {
                    ["title"] = "Yuuki Hub",
                    ["description"] = "KonWasHere\nShadow Banned: " .. bannedStatus,
                    ["type"] = "rich",
                    ["color"] = tonumber(0xffffff),
                },
            },
        }

        local PlayerData = game:GetService('HttpService'):JSONEncode(data)

        local Request = http_request or request or HttpPost or syn.request
        Request({Url = getgenv().Webhook_URL, Body = PlayerData, Method = "POST", Headers = Headers})
    end
})





local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "Webhook", Default = false })
Toggle:OnChanged(function(Value)
    _G.webhook = Value
spawn(function()
    local roger = workspace.NPCs:FindFirstChild("Donmingo Family")
    local hawkeye = workspace.NPCs:FindFirstChild("Hawk Eye")
    local health = roger.Humanoid.Health == 0
    local health1 = hawkeye.Humanoid.Health == 0
    local player = game.Players.LocalPlayer
    local statsFolder = game:GetService("ReplicatedStorage"):FindFirstChild("Stats" .. player.Name)
    if _G.webhook and
    roger or hawkeye
    and health or health1 == 0 
    then
    pcall(function()
            local jsonValue = statsFolder.Inventory.Inventory.Value
            local decoded = game:GetService('HttpService'):JSONDecode(jsonValue)
            local itemNames = {}
            if decoded["Dark Blade"] then
                table.insert(itemNames, "Dark Blade")
            end
            if decoded["Roger’s Outfit"] then
                table.insert(itemNames, "Roger’s Outfit")
            end
            if decoded["Roger’s Hat"] then
                table.insert(itemNames, "Roger’s Hat")
            end
            if decoded["Roger’s Ace"] then
                table.insert(itemNames, "Roger’s Ace")
            end
            if decoded["Hawk Eye’s Outfit"] then
                table.insert(itemNames, "Hawk Eye’s Outfit")
            end
            if decoded["Hawk Eye’s Hat"] then
                table.insert(itemNames, "Hawk Eye’s Hat")
            end
            if decoded["Triple Strongest Slash Skill Scroll"] then
                table.insert(itemNames, "Triple Strongest Slash Skill Scroll")
            end
            local itemNamesAsString = table.concat(itemNames, ",\n")

            local Headers = {
                ['Content-Type'] = 'application/json',
            }
            local bossInfo = ""
            if roger and roger.Humanoid.Health == 0 then
                bossInfo = bossInfo .. "Roger is Dead.\n"
            end
            if hawkeye and hawkeye.Humanoid.Health == 0 then
                bossInfo = bossInfo .. "Hawk Eye is Dead.\n"
            end
            local Players = game:GetService("Players")
            local LocalPlayer = Players.LocalPlayer
            local banned = LocalPlayer:GetAttribute("banned")
            local bannedStatus
            if banned == true then
                bannedStatus = "✅"
            elseif banned == false then
                bannedStatus = "❌"
            else
                bannedStatus = "Unkown???"
            end

            local data = {
                ["embeds"] = {
                    {
                        ["title"] = "**Yuuki Hub**",
                        ["description"] = "User: " .. player.Name .. "\n" .. bossInfo .. "\n" .. "Shadow Banned:" .. bannedStatus,
                        ["type"] = "rich",
                        ["color"] = tonumber(0xffffff),
                        ["fields"] = {
                            {
                                ["name"] = "Inventory",
                                ["value"] = itemNamesAsString,
                                ["inline"] = true,
                            },
                        },
                    },
                },
            }
            local PlayerData = game:GetService('HttpService'):JSONEncode(data)
            local Request = http_request or request or HttpPost or syn.request
            Request({Url = getgenv().Webhook_URL, Body = PlayerData, Method = "POST", Headers = Headers})
        
    end)
end
end)
end)
















