local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "Auto Roger/Mihawk", Default = false })

Toggle:OnChanged(function(Value)
    getgenv().AutoBoss = Value
    getgenv().saveena = Value
    if getgenv().AutoBoss then
spawn(function()
    while getgenv().AutoBoss do
        local boss = workspace.NPCs:FindFirstChild("Hawk Eye")
        if boss then
            pcall(function()
                while boss do
                    local setpos = CFrame.new(nil,nil,nil, 1, 0, 0, 0, 0, -1, 0, -1, 0)
                    local getxyzpos = boss.Head.Position
                    local _, _, _, r00, r01, r02, r10, r11, r12, r20, r21, r22 = setpos:GetComponents()
                    if getxyzpos then
                        local getpos = CFrame.new(getxyzpos.X, getxyzpos.Y + 10, getxyzpos.Z)
                        local args = {
                            [1] = "Ice Partisan",
                            [2] = {
                                ["cf"] = CFrame.new(getxyzpos.X, getxyzpos.Y + 10, getxyzpos.Z, r00, r01, r02, r10, r11, r12, r20, r21, r22)
                            }
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Skill"):InvokeServer(unpack(args))
                        task.wait(0.1)
                    end
                end
            end)
        end
        task.wait(0.1)
    end
end)

