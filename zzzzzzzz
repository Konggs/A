local itemsToEquip = {
    "Venom",
    "Ope",
    "Mochi",
    "Tori",
    "Pika",
    "Magu",
    "Kage",
    "Yami",
    "Mera",
    "Paw",
    "Hie",
    "Gura",
    "Zushi",
    "Goro",
    "Suna",
    "Ito",
    "Gomu",
    "Spin",
    "Heal",
