    "Spring",
    "Suke",
    "Horo",
    "Kilo",
    "Mero",
    "Bari",
    "Buddha"
}

local lastBackpackSize = 0
local lastToolAdded = ""

function autoEquipItems()

        local player = game.Players.LocalPlayer
        local backpack = player.Backpack

        local currentBackpackSize = #backpack:GetChildren()

        if currentBackpackSize > lastBackpackSize then
            for _, itemName in ipairs(itemsToEquip) do
                local item = backpack:FindFirstChild(itemName)
                if item and item:IsA("Tool") and item.Name > lastToolAdded then
                    player.Character.Humanoid:EquipTool(item)
                    lastToolAdded = item.Name   
                    wait(1)
                    break
                end
            end

            local melee = backpack:FindFirstChild("Melee")
            if melee and melee:IsA("Tool") then
                player.Character.Humanoid:EquipTool(melee)
            end
        end

        lastBackpackSize = currentBackpackSize
        wait(2)
    
end







function instant(et)
    for i, x in next, game.Players.LocalPlayer.Character:GetDescendants() do
        if x:IsA("BasePart") and not x.Anchored then
            x.Anchored = et
        end
    end
    for i, x in next, game.Players.LocalPlayer.Character:GetDescendants() do
        if x.Name ~= floatName and x:IsA("BasePart") and x.Anchored then
            x.Anchored = et
        end
    end
end




function autostorefruit()
    local ohBoolean1 = true
    game:GetService("ReplicatedStorage").Events.FruitStorage:InvokeServer(ohBoolean1)
end



local hasRun = false
function DeleteEffect()
    if hasRun then return end
    hasRun = true
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Effects = ReplicatedStorage:FindFirstChild("Effects")
    local HitEffect = Effects:FindFirstChild("HitEffect")
    if HitEffect then
        HitEffect:Destroy()
    end
    local counter = game:GetService("Players")[game:GetService("Players").LocalPlayer.Name].PlayerScripts.ClientMain.Combo
    if counter then
        counter:Destroy()
    end
    if game:GetService("ReplicatedStorage").Effects then
        game:GetService("ReplicatedStorage").Effects:Destroy()
    end
    if workspace.Effects.DeadNPCs then
        workspace.Effects.DeadNPCs:Destroy()
    end
    if game:GetService("Players").LocalPlayer.PlayerGui.Notifications then
        game:GetService("Players").LocalPlayer.PlayerGui.Notifications:Destroy()
    end
end








function animationsb()
    local ohTable1 = {
        [1] = "swingsfx",
        [2] = "Melee",
        [3] = 1,
        [4] = "Ground",
        [5] = false,
        [6] = game:GetService("ReplicatedStorage").CombatAnimations.Melee.Punch1,
        [7] = 1.75,
        [8] = 5
    }
    
    game:GetService("ReplicatedStorage").Events.CombatRegister:InvokeServer(ohTable1)
    end




